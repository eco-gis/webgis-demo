{
	"version": 8,
	"name": "waldkauz-overlay",
	"sources": {
		"waldkauz_points": {
			"type": "geojson",
			"data": "/data/waldkauz_location.geojson"
		},
		"waldkauz_buffer_500": {
			"type": "geojson",
			"data": "/data/buffer_500.geojson"
		},
		"waldkauz_buffer_1000": {
			"type": "geojson",
			"data": "/data/buffer_1000.geojson"
		},
		"waldkauz_buffer_2000": {
			"type": "geojson",
			"data": "/data/buffer_2000.geojson"
		}
	},
	"layers": [
		{
			"id": "waldkauz-buffer-2000",
			"type": "fill",
			"source": "waldkauz_buffer_2000",
			"paint": {
				"fill-color": "hsla(289, 19%, 28%, 0.15)",
				"fill-outline-color": "hsl(289, 19%, 28%)"
			}
		},
		{
			"id": "waldkauz-buffer-1000",
			"type": "fill",
			"source": "waldkauz_buffer_1000",
			"paint": {
				"fill-color": "hsla(242, 18%, 39%, 0.22)",
				"fill-outline-color": "hsl(242, 18%, 39%)"
			}
		},
		{
			"id": "waldkauz-buffer-500",
			"type": "fill",
			"source": "waldkauz_buffer_500",
			"paint": {
				"fill-color": "hsla(195, 53%, 43%, 0.30)",
				"fill-outline-color": "hsl(195, 53%, 43%)"
			}
		},
		{
			"id": "waldkauz-points",
			"type": "circle",
			"source": "waldkauz_points",
			"paint": {
				"circle-radius": 6,
				"circle-color": "hsl(153, 57%, 60%)",
				"circle-stroke-width": 2,
				"circle-stroke-color": "hsl(153, 57%, 40%)"
			}
		}
	]
}
